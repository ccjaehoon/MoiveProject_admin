<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.movieadmin.board">
	<insert id="INSERT" parameterType="com.project.movieadmin.board.BoardVO">
		insert into
		board(title, tag, nickname, content, wdate, good, authority, views, commentscount, report)
		values(#{title},#{tag},#{nickname},#{content},#{wdate},#{good},#{authority},#{views},#{commentscount},#{report})
	</insert>

	<update id="UPDATE">
		update board set title=#{title},tag=#{tag},content=#{content},nickname=#{nickname} 
		where board_num=#{board_num}
	</update>

	<delete id="DELETE">
		delete from board where board_num=#{board_num}
	</delete>

	<select id="SELECT_ONE" resultType="com.project.movieadmin.board.BoardVO">
		select * from board where board_num=#{board_num}
	</select>

	<select id="SELECT_ALL" resultType="com.project.movieadmin.board.BoardVO">
		select * from board order by board_num desc
	</select>

	<select id="SELECT_ALL_PAGE_BLOCK"
		resultType="com.project.movieadmin.board.BoardVO">
		
	</select>

	
	<select id="SEARCHLIST_ID"
		resultType="com.project.movieadmin.board.BoardVO">
		
	</select>

	
	<select id="SEARCHLIST_NAME"
		resultType="com.project.movieadmin.board.BoardVO">
		
	</select>

	<select id="SEARCHLIST_PAGE_BLOCK_ID"
		resultType="com.project.movieadmin.board.BoardVO">
		
	</select>

	<select id="SEARCHLIST_PAGE_BLOCK_NAME"
		resultType="com.project.movieadmin.board.BoardVO">
		
	</select>


	<select id="TOTAL_ROWS"	resultType="int">
		
	</select>

	
	<select id="SEARCH_TOTAL_ROWS_ID"	resultType="int">
	
	</select>

	
	<select id="SEARCH_TOTAL_ROWS_NAME"	resultType="int">
		
	</select>

	
	<select id="LOGIN"	resultType="com.project.movieadmin.board.BoardVO">
		
	</select>


</mapper>