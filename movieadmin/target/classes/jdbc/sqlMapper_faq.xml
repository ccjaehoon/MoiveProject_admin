<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.movieadmin.faq">
	<insert id="F_INSERT" parameterType="com.project.movieadmin.faq.FaqVO">
		insert into
		faq(title,nickname,content)
		values(#{title},'admin',#{content})
	</insert>

	<update id="F_UPDATE" parameterType="com.project.movieadmin.faq.FaqVO">
		
	</update>

	<delete id="F_DELETE" parameterType="com.project.movieadmin.faq.FaqVO">
	
	</delete>

	<select id="F_SELECT_ONE" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>

	<select id="F_SELECT_ALL" resultType="com.project.movieadmin.faq.FaqVO">
	select * from faq order by num desc
	</select>

	<select id="F_SELECT_ALL_PAGE_BLOCK" resultType="com.project.movieadmin.faq.FaqVO">
		select * from faq order by faq_num desc limit ${startRow}, ${pageBlock}
	</select>

	
	<select id="F_SEARCHLIST_ID" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>

	
	<select id="F_SEARCHLIST_NAME" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>

	<select id="F_SEARCHLIST_PAGE_BLOCK_ID" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>

	<select id="F_SEARCHLIST_PAGE_BLOCK_NAME" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>


	<select id="F_TOTAL_ROWS"	resultType="int">
		select count(*) total_rows from faq
	</select>

	
	<select id="F_SEARCH_TOTAL_ROWS_ID" resultType="int">
	
	</select>

	
	<select id="SEARCH_TOTAL_ROWS_NAME"	resultType="int">
		
	</select>

	
	<select id="LOGIN" resultType="com.project.movieadmin.faq.FaqVO">
		
	</select>


</mapper>