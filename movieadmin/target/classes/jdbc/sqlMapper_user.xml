<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.movieadmin.user">
	<insert id="INSERT" parameterType="com.project.movieadmin.user.UserVO">
		insert into
		user(user_id,password,nickname,gender,birth,tel,email)
		values(#{user_id},#{password},#{nickname},#{gender},#{birth},#{tel},#{email})
	</insert>

	<update id="UPDATE">
		update user set password=#{password},nickname=#{nickname},gender=#{gender},tel=#{tel},email=#{email} 
		where user_num=#{user_num}
	</update>

	<delete id="DELETE">
	delete from user where user_num=#{user_num}
	</delete>

	<select id="SELECT_ONE" resultType="com.project.movieadmin.user.UserVO">
		
	</select>

	<select id="SELECT_ALL" resultType="com.project.movieadmin.user.UserVO">
	
	</select>

	<select id="SELECT_ALL_PAGE_BLOCK"
		resultType="com.project.movieadmin.user.UserVO">
		
	</select>

	
	<select id="SEARCHLIST_ID"
		resultType="com.project.movieadmin.user.UserVO">
		
	</select>

	
	<select id="SEARCHLIST_NAME"
		resultType="com.project.movieadmin.user.UserVO">
		
	</select>

	<select id="SEARCHLIST_PAGE_BLOCK_ID"
		resultType="com.project.movieadmin.user.UserVO">
		
	</select>

	<select id="SEARCHLIST_PAGE_BLOCK_NAME"
		resultType="com.project.movieadmin.user.UserVO">
		
	</select>


	<select id="TOTAL_ROWS"	resultType="int">
		
	</select>

	
	<select id="SEARCH_TOTAL_ROWS_ID"	resultType="int">
	
	</select>

	
	<select id="SEARCH_TOTAL_ROWS_NAME"	resultType="int">
		
	</select>

	
	<select id="LOGIN"	resultType="com.project.movieadmin.user.UserVO">
		
	</select>


</mapper>