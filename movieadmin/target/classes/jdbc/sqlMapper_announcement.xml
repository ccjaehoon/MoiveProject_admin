  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.movieadmin.announcement">
	<insert id="INSERT" parameterType="com.project.movieadmin.announcement.AnnouncementVO">
		insert into
		user(title,nickname,content,wdate,authority,save_img)
		values(#{title},#{nickname},#{content},#{wdate},#{authority},#{save_img})
	</insert>

	<update id="UPDATE" parameterType="com.project.movieadmin.announcement.AnnouncementVO">
		update user set title=#{title},content=#{content},save_img=#{save_img} 
		where user_num=#{announcement_num}
	</update>

	<delete id="DELETE" parameterType="com.project.movieadmin.announcement.AnnouncementVO">
	delete from user where annoucnement_num=#{annoucnement_num}
	</delete>

